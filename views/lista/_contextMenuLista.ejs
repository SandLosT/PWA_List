<div class="dropdown ms-2">
    <button type="button" class="btn btn-sm" style="border:none" data-bs-toggle="dropdown"
        aria-expanded="false">
        <i class="fa-solid fa-ellipsis-vertical"></i>
    </button>
    <ul class="dropdown-menu">
        <li>
            <button type="button" class="btn dropdown-item">
                <i class="fa-solid fa-share-nodes me-2"></i>
                Compartilhar
            </button>
        </li>
        <li>
            <button type="button" class="btn dropdown-item"
                onclick="abrirModalRenomearLista('<%= id %>')">
                <i class="fa-solid fa-pen me-2"></i>
                Renomear
            </button>
        </li>
        <li>
            <button type="button" class="btn dropdown-item"
                onclick="excluirLista('<%= id %>')">
                <i class="fa-solid fa-trash me-2"></i>
                Excluir
            </button>
        </li>
    </ul>
</div>